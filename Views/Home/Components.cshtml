                            </div>
                            <div class="col-lg-6">
                                <bootstrap-input-group>
                                    <input type="text" class="form-control" placeholder="Username" aria-describedby="basic-addon1b">
                                    <bootstrap-input-group-addon type="button" id="basic-addon1b">
